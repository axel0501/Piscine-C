int	iniz_vettore(int vett[], int rip)
{
	int count = 0;

	while (count < rip)
	{
		vett[count] = 0;
		count++;
	}
	return (vett, rip);
}

int	stampa_vettore(int vett[], int rip)
{
	int count = 0;

	while (count < rip)
	{
		write();
		count++;
	}
	return (vett, rip);
}

int	main(void)
{
    int n = 10;
	int vett[n];
	iniz_vettore(vett, n);
	stampa_vettore(vett, n);
}

